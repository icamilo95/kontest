<br>
<div class="container-fluid">
   
   <div class="col-sm-6">
      <div class="panel panel-default">
         <div class="panel-heading">
            <h3 class="panel-title">Ingresar Codigo</h3>
         </div>
         <div class="panel-body">
            <%= form_tag '/registercode' do %>
               <%= label_tag(:kontestcode, "Codigo:") %><%= text_field_tag :kontestcode  %> -
               <%= text_field_tag :unicode  %>
   
               <%= submit_tag("Enviar") %>
            <% end %>

            <hr>
            <%= link_to "Regresar", root_path, :class => "btn btn-default" %>

         </div>
      </div>
   </div>
</div>